{
  "name": "school",
  "version": "1.0.0",
  "description": "School manager",
  "repository": "git@github.com:Flavien-Pensato/school.github.io.git",
  "author": "Flavien-Pensato <flavien.pensato@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "webpack --config webpack.config.js",
    "lint": "eslint --color --cache 'src/*.js' '{src,config,scripts,tests}/**/*.js'",
    "start": "webpack-dev-server",
    "build": "webpack --config webpack.config.js",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/config/jest.js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/moment/"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
