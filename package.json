{
  "name": "school",
  "description": "School manager",
  "repository": "git@github.com:Flavien-Pensato/school.github.io.git",
  "author": "Flavien-Pensato <flavien.pensato@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@styled-system/should-forward-prop": "^5.1.4",
    "bcryptjs": "^2.4.3",
    "deepmerge": "^4.2.2",
    "little-state-machine": "^3.1.3",
    "mongoose": "^5.10.18",
    "next": "^10.0.3",
    "next-auth": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.12.1",
    "slug": "^4.0.2",
    "styled-system": "^5.1.5",
    "swr": "^0.3.9",
    "theme-ui": "^0.3.4",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@m6web/eslint-plugin": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "prettier": "2.2.1",
    "standard-version": "^9.0.0"
  },
  "scripts": {
    "lint": "eslint --color --cache '*.js' '{pages,config,components,modules,elements,theme}/**/*.js'",
    "test": "jest",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "circle": "circleci config validate -c .circleci/config.yml"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "lerna run lint --parallel",
      "git add"
    ],
    "*.spec.js": [
      "lerna run test --parallel",
      "git add"
    ],
    ".circleci/config.yml": [
      "circleci config validate -c .circleci/config.yml",
      "git add"
    ]
  }
}
