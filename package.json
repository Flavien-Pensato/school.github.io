{
  "name": "school",
  "description": "School manager",
  "repository": "git@github.com:Flavien-Pensato/school.github.io.git",
  "author": "Flavien-Pensato <flavien.pensato@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@emotion/styled-base": "^10.0.15",
    "@styled-system/should-forward-prop": "^5.1.4",
    "classname": "^0.0.0",
    "emotion-theming": "^10.0.27",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "firebase": "^6.4.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "next": "^9.1.4 ",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.2",
    "slug": "^1.1.0",
    "styled-system": "^5.1.4",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@m6web/eslint-plugin": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-remove-debug": "^0.0.4",
    "documentation": "^11.0.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lerna": "^3.13.1",
    "lint-staged": "^9.1.0",
    "prettier": "1.18.2",
    "standard-version": "^6.0.1"
  },
  "scripts": {
    "lint": "eslint --color --cache '*.js' '{pages,config,components,modules,elements,theme}/**/*.js'",
    "test": "jest",
    "dev": "next",
    "build": "next build && next export",
    "start": "next dev",
    "documentation": "lerna run documentation --parallel",
    "circle": "circleci config validate -c .circleci/config.yml"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "lerna run lint --parallel",
      "git add"
    ],
    "*.spec.js": [
      "lerna run test --parallel",
      "git add"
    ],
    ".circleci/config.yml": [
      "circleci config validate -c .circleci/config.yml",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest.js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/moment/"
    ],
    "roots": [
      "<rootDir>/"
    ]
  }
}
