{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "school:start": "cd packages/school && webpack-dev-server",
    "school:build": "cd packages/school && webpack --config webpack.config.js -p",
    "school:test": "cd packages/school && yarn test",
    "school:lint": "cd packages/school && yarn lint",
    "storybook": "start-storybook",
    "start": "npm-run-all --parallel ui:watch school:start",
    "test": "npm-run-all --parallel ui:build school:test",
    "lint": "eslint --color --cache 'packages/**/*.js' '{src,config,scripts,tests}/**/*.js'",
    "build": "npm-run-all ui:build school:build"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./config/jest/setup.js"
  },
  "workspaces": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.1.9",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "webpack": "^4.35.0"
  },
  "dependencies": {
    "@emotion/styled": "^10.0.12",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-grid-system": "^4.4.6"
  }
}
